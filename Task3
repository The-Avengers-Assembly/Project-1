# Class CS2560
# AuthorName: Bryce Lam
# Date: 10/19/22
# Objective: Take two user inputs and compare them if they are equal or not
# Github link: https://github.com/The-Avengers-Assembly/Project-1

.data
msg1: .asciiz"Input your first number: "
msg2: .asciiz"Input your second number: "

equalmsg: .asciiz"User inputs are the same"
unequalmsg: .asciiz"User inputs are different"

.text
main:
	#print msg1
	li $v0, 4
	la $a0, msg1
	syscall
	#get first input
	li $v0, 5
	syscall
	move $s0, $v0
	
	#print msg2
	li $v0, 4
	la $a0, msg2
	syscall
	#get second input
	li $v0, 5
	syscall
	move $s1, $v0
	
	#compare the inputs
	beq $s0, $s1, equals
	bne $s0, $s1, notequals
	
equals:
	#print equalmsg
	li $v0, 4
	la $a0, equalmsg
	syscall
	j exit
	
notequals:
	#print unequalsmsg
	li $v0, 4
	la $a0, unequalmsg
	syscall
	j exit
	
exit:
	li $v0, 10
	syscall
	
